{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "implicit-arrow-linebreak": 0,
    "no-restricted-syntax": 0,
    "comma-dangle": 0,
    "quotes": 0,
    "prefer-regex-literals": 0,
    "indent": 0,
    "operator-linebreak": 0,
    "no-unused-vars": 0,
    "func-names": 0,
    "import/prefer-default-export": 0,
    "eol-last": 0
  }
}
